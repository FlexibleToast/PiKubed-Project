vrrp_instance VI_1 {
        state MASTER
        interface {{ ansible_default_ipv4.interface }}
        virtual_router_id 51
        priority {{ vip_priority }}
        advert_int 1
        authentication {
                auth_type PASS
                auth_pass {{ vip_pass }}
        }
        virtual_ipaddress {
                {{ vip_ip }}
        }
}
